<!-- scholarsphere over ride -->
<!-- the sub on ':' is b.c. jQuery has trouble with '.' or ':' 
on element ids -->
<% if document.has_key?(:noid_s) %>
<%# logger.info document[:generic_file__creator_t].inspect %>
  <li>
  <h2>
    <%= link_to document.fetch(:label_t, ['not found']).first, generic_file_path(document[:noid_s].first) %> 
  </h2>
  <table class="verticalheadings">
    <tr>
      <td rowspan="5">
    <%= render :partial => 'dashboard/_index_partials/thumbnail_display', :locals => {:document=>document} %>
      </td>
      <th>Description:</th>
      <td><%= render_document_show_field_value(:document=>document, :field=>"generic_file__description_display") %></th>
    </tr>
    <tr>
      <th>Contributor:</div>
      <td><%= render_document_show_field_value(:document=>document, :field=>"generic_file__contributor_t") %></td>
    </tr>
    <tr>
      <th>Tags:</th>
      <td><%= render_document_show_field_value(:document=>document, :field=>"generic_file__tag_display") %></td>
    </tr>
    <tr>
      <th>Date Modified:</th>
      <td><%= document[:system_modified_dt] %></td>
    </tr>
    <tr>
      <th>Date Created:</th>
      <td><%= render_document_show_field_value(:document=>document, :field=>"generic_file__date_created_display") %></td>
    </tr>

  </table>
<% end %>
