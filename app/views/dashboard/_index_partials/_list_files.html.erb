<!-- scholarsphere over ride -->
<!-- the sub on ':' is b.c. jQuery has trouble with '.' or ':' 
on element ids -->
<% if document.has_key?(:noid_s) %>
<tr id="document_<%= document[:noid_s].first.gsub(":", "_") %>" class="<%= cycle("","zebra") %>"> 
  <td width="3%"><%= check_box_tag :generic_file, document[:id] %></td> 
  <td width="42%">
    <a href="#" id="expand_<%= document[:noid_s].first.gsub(":", "_") %>" class="itemicon itemexpand">expand</a>
    <%= render :partial => 'dashboard/_index_partials/thumbnail_display', :locals => {:document=>document} %>
    <%= link_to document.fetch(:label_t, ['not found']).first, generic_file_path(document[:noid_s].first) %> <br />
    <%= document[:noid_s].first %>
  </td>
  <td width="15%"><%= document.fetch(:system_create_dt,'').to_time.strftime("%m/%d/%Y") %> </td>
  <td width="12%">
    <% if !document[:edit_access_group_t].nil? %>
      Groups: <%= render_document_show_field_value(:document=>document, :field=>"edit_access_group_t") %> 
      <br />
    <% end %>
      Users: <%= render_document_show_field_value(:document=>document, :field=>"edit_access_person_t") %> 
  </td>
  <td width="5%" align="center">
    <% if document[:read_access_group_t].nil? or (document[:read_access_group_t] & ['public']).empty? %>
      <a href="/" class="itemicon itemprivate">private</a>
    <% else %>
      <a href="/" class="itemicon itempublic">public</a>
    <% end %>
  </td>
  <td width="23%" align="center">
    <a href="/" class="itemicon itemcode">get code</a>
    <a href="/" class="itemicon itemedit">edit</a>
    <a href="/" class="itemicon itemdownload">download</a>
  </td>
</tr>
<tr class="hidden" id="detail_<%= document[:noid_s].first.gsub(":", "_") %>"> <!--  document detail"> -->
  <td colspan="6">
    <table class="expanded-details">
      <tr>
        <td><strong>Title:</strong>
          <%= link_to render_document_show_field_value(:document=>document, :field=>"generic_file__title_t"), generic_file_path(document[:noid_s].first) %> </td>
        <td><strong>Format:</strong><%= render_document_show_field_value(:document=>document, :field=>"format_label_t") %> </td>
      </tr>
      <tr>
        <td><strong>Creator:</strong>
        </td>
        <td><strong>Last Audit:</strong>
        </td>
      </tr>
          
    </table>
  </td>
</tr>
<% end %>
